	var g = function() {
		$(i.get(e)).removeClass("current");
		$(i.get(c)).addClass("current");
		f.stop(true, false).animate({
			"margin-left": (0 - c) * window._currentWidth + "px"
		},
		200);
		e = c;
		var l = $(b.get(e));
